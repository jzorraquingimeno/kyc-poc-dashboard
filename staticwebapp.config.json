{
  "routes": [
    {
      "route": "/login",
      "rewrite": "/index.html"
    },
    {
      "route": "/dashboard",
      "rewrite": "/index.html"
    },
    {
      "route": "/ticket/*",
      "rewrite": "/index.html"
    },
    {
      "route": "/investigation/*",
      "rewrite": "/index.html"
    },
    {
      "route": "/investigation-report/*",
      "rewrite": "/index.html"
    },
    {
      "route": "/*",
      "rewrite": "/index.html"
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/static/css/*", "/static/js/*", "/static/media/*", "/*.json", "/*.ico"]
  },
  "mimeTypes": {
    ".json": "application/json"
  },
  "globalHeaders": {
    "Cache-Control": "no-cache, no-store, must-revalidate",
    "Pragma": "no-cache",
    "Expires": "0"
  },
  "responseOverrides": {
    "404": {
      "rewrite": "/index.html",
      "statusCode": 200
    }
  }
}